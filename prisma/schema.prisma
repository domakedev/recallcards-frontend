generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       Int    @id @default(autoincrement())
  email    String @unique @db.VarChar
  password String @db.VarChar
}

model decks {
  id    Int    @id @default(autoincrement())
  name  String @unique @db.VarChar
  image String @db.VarChar
}
